@article{Balister05,
 ISSN = {00018678},
 URL = {http://www.jstor.org/stable/30037313},
 abstract = {Let P be a Poisson process of intensity one in a square $S_n$ of area n. We construct a random geometric graph $G_n,k$ by joining each point of P to its k $\equiv$ k(n) nearest neighbours. Recently, Xue and Kumar proved that if k $\leq$ 0.074 log n then the probability that $G_n,k$ is connected tends to 0 as n $\rightarrow \infty$ while, if k $\geq$ 5.1774 log n, then the probability that $G_n,k$ is connected tends to 1 as n $\rightarrow \infty$. They conjectured that the threshold for connectivity is k = (1 + o(1)) log n. In this paper we improve these lower and upper bounds to 0.3043 log n and 0.5139 log n, respectively, disproving this conjecture. We also establish lower and upper bounds of 0.7209 log n and 0.9967 log n for the directed version of this problem. A related question concerns coverage. With $G_n,k$ as above, we surround each vertex by the smallest (closed) disc containing its k nearest neighbours. We prove that if k $\leq$ 0.7209 log n then the probability that these discs cover $S_n$ tends to 0 as n $\rightarrow \infty$ while, if k $\geq$ 0.9967 log n, then the probability that the discs cover $S_n$ tends to 1 as n $\rightarrow \infty$.},
 author = {Paul Balister and Bela Bollobas and Amites Sarkar and Mark Walters},
 journal = {Advances in Applied Probability},
 number = {1},
 pages = {1--24},
 publisher = {Applied Probability Trust},
 title = {Connectivity of Random k-Nearest-Neighbour Graphs},
 volume = {37},
 year = {2005}
}
