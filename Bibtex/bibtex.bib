@Article{Vaidya1991,
author="Vaidya, Pravin M.",
title="A sparse graph almost as good as the complete graph on points in K dimensions",
journal="Discrete {\&} Computational Geometry",
year="1991",
month="Sep",
day="01",
volume="6",
number="3",
pages="369--381",
abstract="A setV ofn points ink-dimensional space induces a complete
                  weighted undirected graph as follows. The points are
                  the vertices of this graph and the weight of an edge
                  between any two points is the distance between the
                  points under someLp metric. Let $\epsilon$≤1 be an
                  error parameter and letk be fixed. We show how to
                  extract inO(n logn+$\epsilon$ −k log(1/$\epsilon$)n)
                  time a sparse subgraphG=(V, E) of the complete graph
                  onV such that: (a) for any two pointsx, y inV, the
                  length of the shortest path inG betweenx andy is at
                  most (1+∈) times the distance betweenx andy, and
                  (b)|E|=O($\epsilon$−kn).",
issn="1432-0444",
doi="10.1007/BF02574695",
url="https://doi.org/10.1007/BF02574695"
}

